{
  "name": "boilerplate",
  "version": "1.0.0",
  "description": "A boilerplate for React front-end (with Babel and Webpack), Express server, and PostgreSQL database",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "react-dev": "./node_modules/.bin/webpack -d -w",
    "dev": "concurrently --kill-others-on-fail \"npm run server\" \"npm run react-dev\"",
    "server": "nodemon ./server/index.js",
    "start": "webpack && node ./server/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nicole-nguyen1/boilerplate.git"
  },
  "author": "Nicole Nguyen",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/nicole-nguyen1/boilerplate/issues"
  },
  "homepage": "https://github.com/nicole-nguyen1/boilerplate#readme",
  "dependencies": {
    "babel-core": "^6.26.3",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "body-parser": "^1.18.3",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "express-session": "^1.15.6",
    "knex": "^0.15.2",
    "passport": "^0.4.0",
    "pg": "^7.4.3",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "webpack": "^4.17.1"
  },
  "devDependencies": {
    "babel-eslint": "^9.0.0",
    "babel-loader": "^7.1.5",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "concurrently": "^3.6.1",
    "eslint": "^5.5.0",
    "eslint-loader": "^2.1.0",
    "webpack-cli": "^3.1.0"
  }
}
